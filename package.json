{
  "name": "v-satori",
  "version": "0.1.9",
  "packageManager": "pnpm@7.32.5",
  "description": "Generate a satori-friendly VDOM from a Vue component.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/v-satori#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/v-satori.git"
  },
  "bugs": "https://github.com/wobsoriano/v-satori/issues",
  "keywords": [
    "vue",
    "satori",
    "nuxt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./nuxt": {
      "types": "./nuxt/module.d.ts",
      "import": "./nuxt/module.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "nuxt"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "dev:vercel:deploy": "rm -rf .vercel && pnpm --filter playground build && mv ./playground/.vercel ."
  },
  "peerDependencies": {
    "satori": "^0.9.0",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "satori-html": "^0.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@nuxt/kit": "^3.5.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "node-fetch-native": "^1.1.1",
    "pnpm": "^7.32.5",
    "satori": "^0.9.1",
    "tsup": "6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "vue": "^3.3.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
